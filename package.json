{
  "name": "@harnyk/dynalist-api",
  "version": "1.0.0",
  "description": "TypeScript library for Dynalist API",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf lib",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest --coverage"
  },
  "keywords": [
    "dynalist",
    "api",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.6.2",
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "dotenv": "^17.2.2",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
}
